<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="jp.go.meti.drone.relatedpartiesnotice.tenant.repository.mapper.NotificationTargetInfoRepository">

	<!--　周知先情報　-->
	<select id="selectByPrimaryKey"
		resultType="jp.go.meti.drone.relatedpartiesnotice.tenant.repository.entity.NotificationTargetInfoEntity">
		select 
		 *
	    from 
	     notification_target_info 
	    where
		 operator_id=#{operatorId}
	</select>
</mapper>